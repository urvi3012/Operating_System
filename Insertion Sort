#include<iostream>
using namespace std;

void insertionSort(int a[], int n)
{
   int key;
   for (int i = 1; i < n; i++)
   {
       key = a[i];
       int j = i-1;
       while (j >= 0 && a[j] > key)
       {
           a[j+1] = a[j];
           j = j-1;
       }
       a[j+1] = key;
   }
}

int main()
{   cout<<endl<<"        INSERTION SORT";
    int n;
    cout<<endl<<"enter size of array ";
    	cin>>n;
    int a[n]l;
    cout<<endl<<"enter elememts ";
    for(int i=0;i<n;i++)
        cin>>a[i]; 
    insertionSort(a, n);
    cout<<endl<<"sorted elememts";
    for(int i=0;i<n;i++)
    cout<<" "<<a[i];  
    return 0;
}
